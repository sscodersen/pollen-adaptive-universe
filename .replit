modules = ["nodejs-20", "python-3.12", "web"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "vm"
run = ["node", "start.cjs"]
build = ["npm", "run", "build"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "node start.cjs"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 3001

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8080
externalPort = 8080

[[ports]]
localPort = 8081
externalPort = 8081

[[ports]]
localPort = 33593
externalPort = 3000

[[ports]]
localPort = 34965
externalPort = 3002

[agent]
integrations = ["javascript_websocket:1.0.0"]
expertMode = true
